{
  "name": "@fa-repo/shard-docs",
  "version": "6.0.3",
  "description": "A doc manager written in React for organising and viewing MDX files.",
  "author": "fa-repo",
  "license": "MIT",
  "repository": "fa-repo/shard-docs",
  "bin": {
    "shard-docs": "bin/shard-docs.js"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "postbuild": "yarn build-ts",
    "watch": "node scripts/watch.mjs",
    "build-ts": "tsc src/index.ts src/types/shard-docs.d.ts  --declaration --emitDeclarationOnly --jsx react --skipLibCheck --esModuleInterop --outDir dist",
    "docs:deploy": "cd docs && yarn && yarn build && npx gh-pages -d dist --git $(which git)",
    "preversion": "yarn test && yarn build",
    "postpublish": "git push --tags",
    "analyse": "yarn build && npx source-map-explorer ./dist/index.js",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "classnames": "^2.2.6",
    "esbuild": "^0.15.1",
    "esbuild-sass-plugin": "^2.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^0.11.2",
    "postcss": "^8.4.16",
    "prism-react-renderer": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-element-to-jsx-string": "^15.0.0",
    "react-frame-component": "5.2.3",
    "react-router-dom": "6.3.0",
    "slugify": "^1.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "jest": "^28.1.3"
   
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  }
}
