{
  "name": "@fa-repo/shard-docs",
  "version": "4.1.0",
  "description": "A concise / extendable react component for handling documentation",
  "author": "fa-repo",
  "license": "",
  "repository": "fa-repo/shard-docs",
  "main": "dist/shard-docs.js",
  "module": "dist/shard-docs.es.js",
  "jsnext:main": "dist/shard-docs.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npx cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "npx react-scripts test --env=jsdom",
    "build": "npx rollup -c",
    "watch": "npx rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd docs && npm install && npm run production",
    "deploy": "npx gh-pages -d docs/public --git $(which git)",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "prepublishOnly": "npm run build",
    "postpublish": "git push --tags",
    "analyse": "npm run build && npx source-map-explorer ./dist/shard-docs.js",
    "version": "node -p \"require('./package.json').version\""
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/rollup": "^4.3.2",
    "boxicons": "^2.0.3",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.12.0",
    "remarkable": "^2.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^2.2.2",
    "slugify": "^1.3.4",
    "source-map-explorer": "^2.0.1",
    "uniqid": "^5.0.3"
  },
  "peerDependencies": {
    "prop-types": "^15",
    "react": "^16",
    "react-dom": "^16",
    "react-router-dom": "^5"
  }
}
