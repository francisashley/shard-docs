    "dist"
  ],
  "dependencies": {
    "markdown-it": "^9.0.1",
    "markdown-it-toc-and-anchor": "^4.2.0",
    "node-sass": "^4.12.0",
    "raw.macro": "^0.3.0",
    "slugify": "^1.3.4"
