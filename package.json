{
  "name": "@fa-repo/shard-docs",
  "version": "6.0.2",
  "description": "A doc manager written in React for organising and viewing MDX files.",
  "author": "fa-repo",
  "license": "MIT",
  "repository": "fa-repo/shard-docs",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "npx rollup -c -w",
    "build": "npx rimraf ./dist && npx rollup -c",
    "build-ts": "tsc",
    "docs:deploy": "cd docs && yarn && yarn build && npx gh-pages -d dist --git $(which git)",
    "preversion": "yarn test && yarn build",
    "postpublish": "git push --tags",
    "analyse": "yarn build && npx source-map-explorer ./dist/index.js",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.18.6",
    "@fa-repo/base-react": "^2.2.1",
    "@mdx-js/react": "^1.5.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@svgr/rollup": "^4.3.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.1",
    "@types/prop-types": "15.7.1",
    "@types/react": "^18.0.15",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "github-markdown-css": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "immutable": "^4.0.0-rc.12",
    "ionicons": "^4.6.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^0.11.2",
    "jest-localstorage-mock": "^2.4.0",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.12.0",
    "postcss-import": "^12.0.1",
    "prism-react-renderer": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-frame-component": "5.2.3",
    "react-router-dom": "6.3.0",
    "remarkable": "^2.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^2.2.2",
    "slugify": "^1.3.4",
    "source-map-explorer": "^2.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0"
  }
}
